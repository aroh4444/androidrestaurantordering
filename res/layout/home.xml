<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
android:id="@+id/widget28"
android:layout_width="fill_parent"
android:layout_height="fill_parent"
xmlns:android="http://schemas.android.com/apk/res/android"
>
<TableRow
android:id="@+id/ordermealrow"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:orientation="horizontal"
android:layout_above="@+id/viewordersrow"
android:layout_alignParentLeft="true"
>
<ImageButton
android:id="@+id/ordericon"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:src="@drawable/ordericon"
>
</ImageButton>
<TextView
android:id="@+id/ordermealtext"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Order Meal"
>
</TextView>
</TableRow>
<TableRow
android:id="@+id/pagemywaiterrow"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:orientation="horizontal"
android:layout_centerVertical="true"
android:layout_alignParentLeft="true"
>
<ImageButton
android:id="@+id/pagewaitericon"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:src="@drawable/pagewaitericon"
>
</ImageButton>
<TextView
android:id="@+id/pagemywaitertext"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Page my Waiter"
>
</TextView>
</TableRow>
<TableLayout
android:id="@+id/widget29"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:orientation="vertical"
android:stretchColumns="1"
android:layout_alignParentTop="true"
android:layout_centerHorizontal="true"
>
</TableLayout>
<TableRow
android:id="@+id/refilldrinksrow"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:orientation="horizontal"
android:layout_below="@+id/pagemywaiterrow"
android:layout_alignParentLeft="true"
>
<ImageButton
android:id="@+id/refilldrinksicon"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:src="@drawable/refilldrinksicon"
>
</ImageButton>
<TextView
android:id="@+id/refilldrinkstext"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Refill Drinks"
>
</TextView>

</TableRow>
<TableRow
android:id="@+id/viewordersrow"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:orientation="horizontal"
android:layout_above="@+id/pagemywaiterrow"
android:layout_alignParentLeft="true"
>
<ImageButton
android:id="@+id/viewordersicon"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:src="@drawable/viewordersicon"
>
</ImageButton>
<TextView
android:id="@+id/vieworderstext"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="View Orders"
>
</TextView>
</TableRow>
<TableRow
android:id="@+id/paybillrow"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:orientation="horizontal"
android:layout_below="@+id/refilldrinksrow"
android:layout_alignParentLeft="true"
>
<ImageButton
android:id="@+id/paybillicon"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:src="@drawable/paybillicon"
>
</ImageButton>
<TextView
android:id="@+id/paybilltext"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Pay my Bill"
>
</TextView>
</TableRow>
<TableRow
android:id="@+id/exitrow"
android:layout_height="wrap_content"
android:orientation="horizontal"
android:layout_alignParentBottom="true"
android:layout_alignLeft="@+id/widget29"
android:layout_alignParentRight="true" android:layout_width="80dip">
<ImageButton
android:id="@+id/exiticon"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:src="@drawable/exiticon"
>
</ImageButton>
<TextView
android:id="@+id/exittext"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Exit app"
>
</TextView>
</TableRow>
</RelativeLayout>
